{
	"name": "Jakcodex/Muledump CORS Adapter",
	"version": "0.0.1",
	"manifest_version": 2,
	"description": "Manages necessary CORS headers for usage of Jakcodex/Muledump.",
	"background": {
		"scripts": ["background.js"]
	},
	"browser_action": {
		"default_icon": "on.png",
		"default_title": "Jakcodex/Muledump CORS Adapter",
		"default_popup": "popup.html"
	},
	"permissions": [
		"storage",
		"webRequest",
		"webRequestBlocking",
		"<all_urls>"
	],
	"web_accessible_resources": [
		"on.png","off.png"
	],
	"icons": {
		"16": "icon-16.png",
		"32": "icon-32.png",
		"48": "icon-48.png",
		"128": "icon-128.png"
	}
}